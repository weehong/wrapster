{
  "name": "wrapster",
  "private": true,
  "version": "1.0.0",
  "description": "A packaging tracking application that scans waybills and product barcodes, tracks packaging details, and exports reports to PDF or Excel",
  "type": "module",
  "keywords": [
    "packaging",
    "tracking",
    "waybill",
    "barcode",
    "scanner",
    "inventory",
    "pdf-export",
    "excel-export",
    "reporting"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:migrate": "tsx scripts/migrate-products.ts",
    "db:seed": "tsx scripts/seed-products.ts"
  },
